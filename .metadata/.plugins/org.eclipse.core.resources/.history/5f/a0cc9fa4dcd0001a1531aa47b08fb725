package com.saiparesh;

public class PassingDataBetweenMethods {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number = 4;
		System.out.println("number = "+number);
		newNumber(5);
		System.out.println("number = " + number);
		
	}
	
	public static void newNumber(int number) {
		number = 10;
	}

}
